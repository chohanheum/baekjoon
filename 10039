#include <stdio.h>
#pragma warning (disable:4996)

int main()
{
    int score;
    int sum = 0;
    int avg = 0;

    for (int i = 0; i < 5; i++)
    {
        scanf_s("%d", &score);


        if (score < 40)
        {
            score = 40;
        }
       
        
            sum = sum + score;
        
    }

        avg = sum / 5;
        printf("%d", avg);
        
        return 0;

}
